{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/go",
      "src": "api/webhook.go",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.go"
    },
    {
      "src": "/api/redis",
      "dest": "/api/redis.go"
    }
  ]
}